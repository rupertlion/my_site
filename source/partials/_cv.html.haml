.cv
    %h2 MY HISTORY
    .linecenter
    .cv_grid
        - data.cv.each do |cv|
            .cv_card{id:"#{cv.id}"}
                %h4= cv[:role]
                %h2= cv[:description]
                %img{src: cv[:logo] }
                    #mycvphoto
                        %img{src: cv[:image] }
                    %h3= cv[:years]